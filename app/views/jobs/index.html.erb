<p id="notice"><%= notice %></p>

<h1>Jobs</h1> <%= Time.now %>

<%= turbo_stream_from :jobs %>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody id="jobs">
    <%= render @jobs %>
  </tbody>
</table>

<br>

<%= turbo_frame_tag 'new_job', src: new_job_path, target: :_top %>
